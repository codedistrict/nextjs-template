{
  "name": "Next-Boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "sh bin/dev.sh",
    "build": "sh bin/build.sh",
    "start": "NODE_OPTIONS='-r @newrelic/next' next start",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "test": "jest --watch",
    "test-all": "jest",
    "snapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage",
    "find:unused": "next-unused",
    "depcheck": "npx depcheck",
    "prepare": "husky install"
  },
  "next-unused": {
    "alias": {},
    "debug": true,
    "include": [
      "src"
    ],
    "exclude": [
      "__test__"
    ],
    "entrypoints": [
      "src/app"
    ]
  },
  "dependencies": {
    "@ag-grid-community/styles": "^30.2.1",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.19",
    "@newrelic/next": "^0.7.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@types/react-date-range": "^1.4.8",
    "ag-grid-community": "^30.2.1",
    "ag-grid-react": "^30.2.1",
    "braintree": "^3.19.0",
    "braintree-web-drop-in-react": "^1.2.1",
    "cookies-next": "^4.1.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "eslint-plugin-filenames-simple": "^0.9.0",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "newrelic": "^11.6.0",
    "next": "14.0.4",
    "next-translate": "^2.6.2",
    "next-unused": "^0.0.6",
    "next13-progressbar": "^1.1.1",
    "nextjs-toploader": "^1.6.4",
    "prettier": "^3.1.0",
    "react": "^18",
    "react-date-range": "^1.4.0",
    "react-dom": "^18",
    "react-easy-crop": "^5.0.2",
    "react-hot-toast": "^2.4.1",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0",
    "swr": "^2.2.4",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/braintree": "^3.3.11",
    "@types/braintree-web-drop-in": "^1.39.3",
    "@types/jest": "^29.5.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "depcheck": "^1.4.7",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "eslint-plugin-eslint-plugin": "^5.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "next-translate-plugin": "^2.6.2",
    "typescript": "^5"
  }
}
